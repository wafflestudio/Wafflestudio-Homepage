<script id="project_template" type="text/x-jquery-tmpl">
  <div class="project_popup">
    <h1 class="name">${name}</h1>
    <div class="images">
      <ul class="screenshots">
      {{each(i, url) screenshot_urls}}
        <li><img src="${url}" /></li>
      {{/each}}
      </ul>
      <ul class="thumbnails">
      {{each(i, url) thumbnail_urls}}
        <li><img src="${url}" /></li>
      {{/each}}
      </ul>
    </div>
    <div class="text_content">
      <span class="since">since ${since}</span>
      <p class="description">
        ${description}
      </p>
    </div>
    <ul class="members">
    {{each(i, m) members}}
      <li data-id="${m.member.id}">
        {{if m.member.thumbnail_url}}
        <img src="${m.member.thumbnail_url}" />
        {{/if}}
        <div class="caption">
        ${m.member.name}
        </div>
      </li>
    {{/each}}
    </ul>
    <div class="nav">
      <div class="prev"></div>
      <div class="next"></div>
    </div>
  </div>
</script>
